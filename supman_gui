	game.Players.LocalPlayer.Character.Head.OverheadGui.Labels.AFK:Destroy()
end)

Lag.Name = "Lag"
Lag.Parent = Main
Lag.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
