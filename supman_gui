Lag.Font = Enum.Font.SourceSans
Lag.Text = "Hide Tool"
Lag.TextColor3 = Color3.fromRGB(255, 255, 255)
Lag.TextSize = 14.000
Lag.MouseButton1Down:Connect(function()
	local lp = game.Players.LocalPlayer
local char = lp.Character

char:FindFirstChildOfClass("Tool").RequiresHandle = false
char:FindFirstChildOfClass("Tool").Handle:Destroy()
end)

