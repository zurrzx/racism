local BC = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local Ultimate = Instance.new("TextButton")
local Semi = Instance.new("TextButton")
local Arrest = Instance.new("TextButton")
local Lag = Instance.new("TextButton")
local HideName = Instance.new("TextButton")
local G36 = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")
local Reach = Instance.new("TextButton")

--Properties:

BC.Name = "BC"
BC.Parent = game.CoreGui
BC.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Main.Name = "Main"
Main.Parent = BC
Main.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
Main.BorderSizePixel = 0
Main.Position = UDim2.new(0.75230062, 0, 0.57914114, 0)
Main.Size = UDim2.new(0, 229, 0, 243)

Ultimate.Name = "Ultimate"
Ultimate.Parent = Main
Ultimate.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
Ultimate.BorderSizePixel = 0
Ultimate.Position = UDim2.new(0.109170303, 0, 0.156378597, 0)
Ultimate.Size = UDim2.new(0, 77, 0, 26)
Ultimate.Font = Enum.Font.SourceSans
Ultimate.Text = "Ultimate Weapon"
Ultimate.TextColor3 = Color3.fromRGB(255, 255, 255)
Ultimate.TextScaled = true
Ultimate.TextSize = 14.000
Ultimate.TextWrapped = true
Ultimate.MouseButton1Down:Connect(function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/Tommy-IL/misc-scripts/main/UltimateWeapon'),true))()
end)

Semi.Name = "Semi"
Semi.Parent = Main
Semi.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
Semi.BorderSizePixel = 0
Semi.Position = UDim2.new(0.554585159, 0, 0.156378597, 0)
Semi.Size = UDim2.new(0, 77, 0, 26)
Semi.Font = Enum.Font.SourceSans
Semi.Text = "Semi Legit"
Semi.TextColor3 = Color3.fromRGB(255, 255, 255)
Semi.TextSize = 14.000
Semi.MouseButton1Down:Connect(function()
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
   if v:FindFirstChild("GunConfig") then
       local c =  require(v.GunConfig)
       c.MaxAmmo = 21
       c.HitDamage = 40
       c.TeamKill = true
       
            end
        end
end)


