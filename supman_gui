-- Scripts:

local function INUFZC_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	print("Press 1 on your keypad to toggle the gui!")
end
coroutine.wrap(INUFZC_fake_script)()
local function WYAIB_fake_script() -- BC.LocalScript 
	local script = Instance.new('LocalScript', BC)

	function onKeyPress(inputObject, gameProcessedEvent)
		if not gameProcessedEvent then
			if inputObject.KeyCode == Enum.KeyCode.KeypadOne  then 
			script.Parent.Main.Visible = not script.Parent.Main.Visible
			end
		end
	end
	game:GetService("UserInputService").InputBegan:connect(onKeyPress)
end
coroutine.wrap(WYAIB_fake_script)()
